<types namespace="_github">
  <type name="EnablePages" desc="Options to enable pages." noToc>
    <prop string name="branch" default="master">
      The branch, either `master` or `gh-pages`.
    </prop>
    <prop string name="path" default="/docs">
      When setting branch to master, this can be set to `/docs`,
      otherwise it has to be `/`.
    </prop>
  </type>
  <type name="Page" desc="The enabled page object.">
    <prop string name="url">
      `https://api.github.com/repos/github/developer.github.com/pages`
    </prop>
    <prop string name="status">
      `built`
    </prop>
    <prop string name="cname">
      `developer.github.com`
    </prop>
    <prop boolean name="custom_404">
      `false`
    </prop>
    <prop string name="html_url">
      `https://developer.github.com`
    </prop>
    <prop type="{ branch: string, directory: string }" name="source">
      The source.
      ```json
      "source": {
        "branch": "master",
        "directory": "/"
      }
      ```
    </prop>
  </type>
</types>